package e;
import java.util.Scanner;

public class oa {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Ingrese un string xd: ");
		String a = sc.nextLine();
		String[] b = a.split(" "); 
		for(String ab : b) {
			System.out.println(ab);
		}
	}
}
