package oa;
import java.util.Scanner;

public class esa {
	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		String pal = "La lluvia en Sevilla es una maravilla";
		int contador = 0;
		for (int i=0; i< pal.length(); i++){
			System.out.print((int)pal.charAt(i)+" ");
		}
		
	}
}
