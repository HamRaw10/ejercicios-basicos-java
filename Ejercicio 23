package oa;
import java.util.Scanner;

public class esa {
	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		String pal = "La lluvia en Sevilla es una maravilla";
		int contador = 0;
		for(int i = 0; i <= pal.length()-1; i++) {
			char ea = pal.charAt(i);
			if(ea == 'a') {
				ea = 'e';
			}
		}
		System.out.println(pal.replace('a', 'e'));
		
	}
}
